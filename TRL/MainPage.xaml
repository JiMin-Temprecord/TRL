<Page
    x:Class="TRL.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TRL"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" FocusVisualSecondaryBrush="#E5FFFFFF">
        <Button x:Name="PDFPreview" Content="" HorizontalAlignment="Left" Height="450" Margin="50,50,0,0" VerticalAlignment="Top" Width="450" FontSize="36" FontFamily="Myriad Pro" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" Click="PDFPreview_Click" Tapped="PDFPreview_Tapped">
            <Button.Background>
                <ImageBrush ImageSource="/Resources/TempLite-02.png" Stretch="None"/>
            </Button.Background>
        </Button>
        <Button x:Name="ExcelPreview" Content="" HorizontalAlignment="Left" Height="450" Margin="50,550,0,0" VerticalAlignment="Top" Width="450" FontFamily="Myriad Pro" FontSize="36" Click="ExcelPreview_Click" Tapped="ExcelPreview_Tapped">
            <Button.Background>
                <ImageBrush ImageSource="/Resources/TempLite-05.png" Stretch="None"/>
            </Button.Background>
        </Button>

    </Grid>
</Page>
